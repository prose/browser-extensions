{
  "name": "Edit in Prose",
  "description": "Provides an \"Edit in Prose\" link from a hosted file on GitHub.",
  "version": "1.1.2",
  "manifest_version": 2,
  "author": "Tristen Brown",
  "homepage_url": "http://prose.io",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["prose.css"],
      "js": ["prose.js"],
      "run_at": "document_end"
    }
  ],
  "applications": {
    "gecko": {
      "id": "{c515e2d0-4663-11e4-916c-0800200c9a66}",
      "strict_min_version": "53.0"
    }
  }
}
